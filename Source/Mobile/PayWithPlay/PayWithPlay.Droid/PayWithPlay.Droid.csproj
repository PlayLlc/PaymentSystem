<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-android</TargetFramework>
    <OutputType>Exe</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationId>com.paywithplay.dev</ApplicationId>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
    <SupportedOSPlatformVersion>28.0</SupportedOSPlatformVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>False</Optimize>
    <EmbedAssembliesIntoApk>False</EmbedAssembliesIntoApk>
    <PublishTrimmed>True</PublishTrimmed>
  </PropertyGroup>
  <ItemGroup>
    <AndroidJavaSource Remove="Adapters\**" />
    <AndroidLibrary Remove="Adapters\**" />
    <AndroidNativeLibrary Remove="Adapters\**" />
    <Compile Remove="Adapters\**" />
    <EmbeddedResource Remove="Adapters\**" />
    <JavaSourceJar Remove="Adapters\**" />
    <None Remove="Adapters\**" />
    <ProguardConfiguration Remove="Adapters\**" />
    <TransformFile Remove="Adapters\**" />
  </ItemGroup>
  <ItemGroup>
    <TrimmerRootDescriptor Include="LinkDescription.xml" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="DeleteBinObjFolders.bat" />
    <None Remove="LinkDescription.xml" />
    <None Remove="Resources\drawable\default_bordered_view.xml" />
    <None Remove="Resources\drawable\enable_settings_button_selector.xml" />
    <None Remove="Resources\drawable\primary_button_background.xml" />
    <None Remove="Resources\drawable\secondary_button_background.xml" />
    <None Remove="Resources\drawable\secondary_button_text_color.xml" />
    <None Remove="Resources\drawable\unelevated_button_text_color.xml" />
    <None Remove="Resources\drawable\verify_email_image.xml" />
    <None Remove="Resources\drawable\verify_phone_number_image.xml" />
    <None Remove="Resources\drawable\welcome_top_logo.xml" />
    <None Remove="Resources\font\poppins_medium.ttf" />
    <None Remove="Resources\font\poppins_regular.ttf" />
    <None Remove="Resources\font\poppins_semibold.ttf" />
    <None Remove="Resources\layout\activity_create_account.xml" />
    <None Remove="Resources\layout\activity_create_account_steps.xml" />
    <None Remove="Resources\layout\activity_enable_device_settings.xml" />
    <None Remove="Resources\layout\activity_generic.xml" />
    <None Remove="Resources\layout\activity_sign_in.xml" />
    <None Remove="Resources\layout\activity_welcome.xml" />
    <None Remove="Resources\layout\fragment_registration_business_name.xml" />
    <None Remove="Resources\layout\fragment_registration_home_or_business_address.xml" />
    <None Remove="Resources\layout\fragment_registration_merchant_type.xml" />
    <None Remove="Resources\layout\fragment_registration_user_address.xml" />
    <None Remove="Resources\layout\fragment_registration_user_name.xml" />
