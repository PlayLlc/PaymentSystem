@using System.Collections.Immutable
@using System.Security.Claims
@using Microsoft.AspNetCore.Authentication

<h2>Claims</h2>

<dl>
    @foreach (Claim claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (KeyValuePair<string, string?> prop in (await Context.AuthenticateAsync()).Properties?.Items ?? ImmutableDictionary<string, string?>.Empty)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>