@using Microsoft.AspNetCore.Authentication
@using System.Collections.Immutable

<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties?.Items ?? ImmutableDictionary<string, string?>.Empty)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>